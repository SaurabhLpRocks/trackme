{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "sourceMap": true,
        "watch": true,
        "removeComments": true,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true
    },
    "exclude": [
        "fonts",
        "images",
        "img",
        "libs", 
        "navigation",
        "node_modules",
        "pagehtmls",
        "typings"
    ]
}